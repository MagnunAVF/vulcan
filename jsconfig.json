{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "#root/*": [
        "./"
      ],
      "#lib/*": [
        "./lib"
      ],
      "#errors": [
        "./lib/errors/base.error.js"
      ],
      "#utils": [
        "./lib/utils/index.js"
      ],
      "#polyfills/*": [
        "./lib/polyfills"
      ],
      "#targets/*": [
        "./lib/targets"
      ],
      "#types": [
        "./lib/types/index.js"
      ],
      "#preset/*": [
        "./lib/presets"
      ],
      "#env": [
        "./lib/env/index.js"
      ],
      "#platform": [
        "./lib/platform/index.js"
      ],
      "#constants": [
        "./lib/constants/index.js"
      ]
    }
  }
}